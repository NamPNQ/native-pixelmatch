{
  "name": "demoApp",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "test": "jest",
    "lint": "eslint .",
    "e2e": "npm-run-all --sequential detox:*",
    "android:build:debug": "npx jetify && cd android && ./gradlew clean && ./gradlew assembleDebug && cd ../",
    "appium:test:android": "npx wdio ./wdio.android.local.conf.js",
    "appium:test:ios": "npx wdio ./wdio.ios.local.conf.js",
    "appium:test:android:watch": "npm run appium:test:android -- --watch",
    "detox:build": "detox build",
    "detox:test": "detox test",
    "postinstall1": "yarn dev-inst && solidarity && yarn link:ios",
    "inst": "yarn add ../",
    "dev-inst": "yarn clean && cp -R ../dist ./module",
    "clean": "rm -rf ./module",
    "link:ios": "cd ios && pod install && cd .."
  },
  "dependencies": {
    "react": "16.13.1",
    "react-native": "0.63.4"
  },
  "devDependencies": {
    "@babel/core": "^7.8.4",
    "@babel/runtime": "^7.8.4",
    "@react-native-community/eslint-config": "^1.1.0",
    "@wdio/appium-service": "^6.10.4",
    "@wdio/cli": "^6.10.5",
    "@wdio/jasmine-framework": "^6.10.4",
    "@wdio/local-runner": "^6.10.5",
    "@wdio/mocha-framework": "^6.10.4",
    "@wdio/spec-reporter": "^6.8.1",
    "@wdio/sync": "^6.10.4",
    "appium": "^1.19.1",
    "babel-jest": "^25.1.0",
    "chai": "^4.2.0",
    "chromedriver": "^87.0.1",
    "eslint": "^6.5.1",
    "jest": "^25.1.0",
    "metro-react-native-babel-preset": "^0.59.0",
    "react-test-renderer": "16.13.1",
    "wd": "^1.13.0",
    "wdio-chromedriver-service": "^6.0.4",
    "webdriverio": "^6.10.5"
  },
  "jest": {
    "preset": "react-native"
  }
}
